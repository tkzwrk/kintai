<h1><%= current_user.name %>さんの勤怠入力画面</h1>

<div class="kintai-main">
  <h2>勤怠情報の入力</h2>

    <%= form_for(@kintaikintai) do |f| %>
    <p>出勤日時入力</p>
      <%= f.datetime_select :sdate %>
      <%= f.submit "出勤時間を入力してクリック" ,class:"btn" %><br>
      <button class="btn"><%= link_to "戻る",root_path, method: :delete, class: 'link'%></button>
    <% end %> 

</div>